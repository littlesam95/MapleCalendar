<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CalendarView">

        <attr name="android:fontFamily" />

        <attr name="calendarViewStyle" format="reference" />

        <attr name="calendarItemViewStyle" format="reference" />

        <attr name="calendarItemHeight" format="dimension" />

        <attr name="calendarItemTextSize" format="dimension" />

        <attr name="calendarItemFontFamily" format="reference|integer" />

    </declare-styleable>

    <declare-styleable name="StarforceView">

        <attr name="starforceViewStyle" format="reference" />

        <attr name="starforceItemViewStyle" format="reference" />

        <attr name="starWidth" format="dimension" />

        <attr name="starHeight" format="dimension" />

        <attr name="starOnColor" format="reference|integer" />

        <attr name="starOffColor" format="reference|integer" />

        <attr name="starBlueColor" format="reference|integer" />

    </declare-styleable>

    <declare-styleable name="HyperStatView">

        <attr name="hyperStatViewStyle" format="reference" />

        <attr name="hyperStatItemViewStyle" format="reference" />

        <attr name="hyperStatItemHeight" format="dimension" />

        <attr name="hyperStatItemTextSize" format="dimension" />

        <attr name="hyperStatItemFontFamily" format="reference|integer" />

    </declare-styleable>

</resources>